{
  "search.exclude": {
    "**/bazel-*": true
  },
  "files.watcherExclude": {
    "**/bazel-*/**": true,

    "**/__pycache__/**": true,
    "**/.mypy_cache/**": true
  },
  "files.associations": {
    "*.axl": "starlark"
  },
  "C_Cpp.workspaceParsingPriority": "medium",
  "prettier.prettierPath": "./bazel-out/bazel_env-opt/bin/tools/bazel_env/bin/prettier",
  "bazel.buildifierFixOnFormat": true,
  "bazel.enableCodeLens": true,
  "bazel.buildifierExecutable": "./bazel-out/bazel_env-opt/bin/tools/bazel_env/bin/buildifier",
  "bazel.lsp.command": "./bazel-out/bazel_env-opt/bin/tools/bazel_env/bin/starpls",
  "bazel.queriesShareServer": false,
  "bazel.lsp.args": [
    "server",
    "--experimental_infer_ctx_attributes",
    "--experimental_use_code_flow_analysis",
    "--experimental_enable_label_completions"
  ],

  "python.languageServer": "None",
  "python.analysis.exclude": ["bazel-*/**"]
}
